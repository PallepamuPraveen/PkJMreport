        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generate Unique Values" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.nio.file.Files;
import java.nio.file.Paths;
import java.nio.charset.StandardCharsets;
import java.util.List;
import java.util.stream.Collectors;
import java.util.stream.IntStream;
// Define the input file path
String inputFilePath = &quot;input_file.txt&quot;;
// Define the output file path for the next sample
String nextOutputPath = &quot;next_sample_values.txt&quot;;
// Read all lines from the input file
List&lt;String&gt; allRecords = Files.readAllLines(Paths.get(inputFilePath), StandardCharsets.UTF_8);
// Filter and collect 50% of the records (e.g., every other record)
List&lt;String&gt; selectedRecords = IntStream.range(0, allRecords.size())
   .filter(i -&gt; i % 2 == 0) // Pick every other record (this gives 50% of the records)
   .mapToObj(allRecords::get)
   .collect(Collectors.toList());
// Write the selected records to the output file
Files.write(Paths.get(nextOutputPath), selectedRecords, StandardCharsets.UTF_8);
log.info(&quot;50% of the records saved to file: &quot; + nextOutputPath);</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
